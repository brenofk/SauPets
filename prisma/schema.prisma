// prisma/schema.prisma

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Usuario {
  id        Int      @id @default(autoincrement())
  nome      String
  cpf       String   @unique
  email     String   @unique
  telefone  String
  senha     String
  createdAt DateTime @default(now())
}
